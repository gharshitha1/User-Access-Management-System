<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
    <display-name>Archetype Created Web Application</display-name>
    
    <servlet>
        <servlet-name>SignUpServlet</servlet-name>
        <display-name>SignUpServlet</display-name>
        <servlet-class>com.management.web.SignUpServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ApprovalServlet</servlet-name>
        <display-name>ApprovalServlet</display-name>
        <servlet-class>com.management.web.ApprovalServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LoginServlet</servlet-name>
        <display-name>LoginServlet</display-name>
        <servlet-class>com.management.web.LoginServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SoftwareServlet</servlet-name>
        <display-name>SoftwareServlet</display-name>
        <servlet-class>com.management.web.SoftwareServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>RequestServlet</servlet-name>
        <display-name>RequestServlet</display-name>
        <servlet-class>com.management.web.RequestServlet</servlet-class>
    </servlet>
    
    <servlet>
        <servlet-name>ApprovalServlet</servlet-name>
        <display-name>ApprovalServlet</display-name>
        <servlet-class>com.management.web.ApprovalServlet</servlet-class>
    </servlet>
    
    <servlet-mapping>
        <servlet-name>SignUpServlet</servlet-name>
        <url-pattern>/signup</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LoginServlet</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SoftwareServlet</servlet-name>
        <url-pattern>/software</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RequestServlet</servlet-name>
        <url-pattern>/request</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ApprovalServlet</servlet-name>
        <url-pattern>/approval</url-pattern>
    </servlet-mapping>
</web-app>